<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.ssi.holiday.service.model.Holiday" />
	<class name="com.ssi.holiday.service.model.impl.HolidayImpl" table="ssi_holiday">
		<id name="holidayId" type="int">
			<generator class="assigned" />
		</id>
		<property name="holidayName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="holidayDate" type="org.hibernate.type.TimestampType" />
	</class>
</hibernate-mapping>